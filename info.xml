<?xml version="1.0"?>
<extension key="com.octopus8.saveasdraft" type="module">
  <file>saveasdraft</file>
  <name>SocialServicesConnect SaveAsDraft</name>
  <description>Creates a new "Save as Draft" button for all activities.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Tey Wei Hao</author>
    <email>weihao@octopus8.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://saveasdraft.octopus8.com</url>
    <url desc="Documentation">http://saveasdraft.octopus8.com</url>
    <url desc="Support">http://saveasdraft.octopus8.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-04-25</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.72</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Saveasdraft</namespace>
    <format>23.02.1</format>
    <angularModule>crmSaveasdraft</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Saveasdraft_Upgrader</upgrader>
</extension>
